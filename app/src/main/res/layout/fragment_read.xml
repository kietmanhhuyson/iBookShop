<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/rectangle_10"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:background="#F8FCFF"
        android:elevation="20dp" />
    <ImageButton
        android:id="@+id/mdi_arrow_l"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/lui"
        android:background="@android:color/transparent"
        android:elevation="20dp"/>
    <ImageButton
        android:id="@+id/Search"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/search"
        android:layout_marginTop="10dp"
        android:layout_toLeftOf="@+id/Aa"
        android:layout_marginRight="15dp"
        android:elevation="20dp"
        android:background="@android:color/transparent"
        android:layout_alignParentTop="true"/>

    <ImageButton
        android:id="@+id/Aa"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="100dp"
        android:layout_marginTop="10dp"
        android:elevation="20dp"
        android:src="@drawable/aa_icon"
        android:background="@android:color/transparent"/>
    <ImageButton
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:src="@drawable/icons8_star_32"
        android:elevation="20dp"
        android:background="@android:color/transparent"
        android:layout_marginTop="4dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="58dp"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bacham"
        android:elevation="20dp"
        android:background="@android:color/transparent"
        android:layout_marginTop="10dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"/>
    <WebView
        android:id="@+id/webview_read"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rectangle_10"/>
</RelativeLayout>